{
    "name": "meuexpansivo-suite",
    "private": true,
    "workspaces": [
        "app-aluno",
        "sistema-gestao"
    ],
    "scripts": {
        "dev": "npm run dev:app",
        "dev:app": "npm run dev --workspace=app-aluno",
        "dev:gestao": "npm run dev --workspace=sistema-gestao",
        "build:app": "npm run build --workspace=app-aluno",
        "build:gestao": "npm run build --workspace=sistema-gestao",
        "build:all": "npm run build --workspaces",
        "install:all": "npm install"
    }
}